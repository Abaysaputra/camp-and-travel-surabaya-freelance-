{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/Assets/home.html"
    },
    {
      "src": "/home",
      "dest": "/Assets/home.html"
    },
    {
      "src": "/login",
      "dest": "/Assets/login.html"
    },
    {
      "src": "/register",
      "dest": "/Assets/register_selection.html"
    },
    {
      "src": "/dashboard-admin",
      "dest": "/Assets/dashboard_admin.html"
    },
    {
      "src": "/dashboard-guide",
      "dest": "/Assets/dashboard_guide.html"
    },
    {
      "src": "/dashboard-customer",
      "dest": "/Assets/dashboard_customer.html"
    },
    {
      "src": "/Assets/(.*)",
      "dest": "/Assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Assets/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
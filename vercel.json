{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/Assets/home.html"
    },
    {
      "source": "/home",
      "destination": "/Assets/home.html"
    },
    {
      "source": "/login",
      "destination": "/Assets/login.html"
    },
    {
      "source": "/register",
      "destination": "/Assets/register_selection.html"
    },
    {
      "source": "/dashboard-admin",
      "destination": "/Assets/dashboard_admin.html"
    },
    {
      "source": "/dashboard-guide",
      "destination": "/Assets/dashboard_guide.html"
    },
    {
      "source": "/dashboard-customer",
      "destination": "/Assets/dashboard_customer.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}